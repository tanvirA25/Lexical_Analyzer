PROGRAM Cylinder
	/*Clean Program testing If-stmt Then-part*/
	INT r, a, h, b; 
	r = 8;
	h = 10;
	a = 0;
	FLOAT surface, volume;
	IF ( r > 5 && h > r) THEN
		volume = 3.14 * r * r * h;
	  	surface = 2 * 3.14 * r * h + 2 * 3.14 * r * r;
	  	PRINT  ("The output results are " , volume, " ", surface, " " , h, " ", r);
	ELSE
		PRINT  ("No computations should be done!" );
	END IF; 
END  PROGRAM
